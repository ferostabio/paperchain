{
  "parser": "babel-eslint",

  "parserOptions": {"ecmaFeatures":
                    {"jsx": true,
                    "esversion":6}},
  "env": {"browser": true,
          "node": true,
          "es6": true },
  "plugins": [ "react" ],
  "extends": ["eslint:recommended",
              "plugin:react/recommended"],
  "rules": { "react/prop-types": [0],
              "no-console": 0,
              "no-var": 1,
              "no-unused-vars": 0 # Need to refactor before adding this one
              }
}
